<h1>Editer le gossip</h1>

<%= form_for @gossip do |f|%>
   <div class='form-group'  >
      <label> Titre </label>
      <%= f.text_field :title, class: 'form-control' %>
   </div>

   <div class='form-group'  >
      <label> Content </label>
      <%= f.text_field :content, class: 'form-control' %>
   </div>

   <div class='form-group'  >
      <button type="submit"> Modifier</button>
   </div>
<%end%>